      'Select '
      '  ID,'
      '  JOURNAL,'
      '  FIO,'
      '  REMARK,'
      '  VERDICT_OGIS0,'
      '  VERDICT_OGIS,'
      '  FIRM,'
      '  TO_FIRM_DATE'
      'from ZMVK_JOURNAL_DETAIL_1 '
      'where'
      '  ID = :ID')
    SelectSQL.Strings = (
      
        'SELECT ID, JOURNAL, FIO, REMARK, VERDICT_OGIS0, VERDICT_OGIS, FI' +
        'RM, TO_FIRM_DATE'
