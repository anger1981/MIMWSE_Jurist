      '  VERDICT_OGIS0 = :VERDICT_OGIS0,'
      '  VERDICT_OGIS = :VERDICT_OGIS,'
      '  FIRM = :FIRM,'
      '  TO_FIRM_DATE = :TO_FIRM_DATE'
