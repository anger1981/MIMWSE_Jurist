  object dsJournalHead: TIBDataSet
    Database = DBForm.IBDatabase
    Transaction = DBForm.IBTransaction
    DeleteSQL.Strings = (
      'delete from JOURNAL'
      'where'
      '  UKEY = :OLD_UKEY')
    InsertSQL.Strings = (
      'insert into JOURNAL'
      '  (ACCOUNT, REG_NUMBER, REG_DATE, REG_DEBT, FIO_ALL, SUIT_DEBT, '
      'DATE_FROM, '
      '   DATE_TILL, LAW_TELL0, LAW_TELL, SUMA, CLOSE_LAW, TO_OGIS, '
      'EXECUTIVE, '
      '   OPEN_OGIS, VERDICT_OGIS, CLOSE_OGIS, DESCRIPTION, LOCKED, '
      'LAST_USER, '
      '   LAST_DATE, LAST_TIME, TO_OGIS_DATE, CLOSE_LAW1, '
      
        'OGIS_RETURN_DATE, TO_OGIS_DATE2, OPEN_OGIS2, CLOSE_OGIS2, OGIS_R' +
        'ETURN_DATE2)'
      'values'
      
        '  (:ACCOUNT, :REG_NUMBER, :REG_DATE, :REG_DEBT, :FIO_ALL, :SUIT_' +
        'DEBT, '
      ':DATE_FROM, '
      
        '   :DATE_TILL, :LAW_TELL0, :LAW_TELL, :SUMA, :CLOSE_LAW, :TO_OGI' +
        'S, '
      ':EXECUTIVE, '
      
        '   :OPEN_OGIS, :VERDICT_OGIS, :CLOSE_OGIS, :DESCRIPTION, :LOCKED' +
        ', '
      ':LAST_USER, '
      '   :LAST_DATE, :LAST_TIME, :TO_OGIS_DATE, :CLOSE_LAW1, '
      
        ':OGIS_RETURN_DATE, :TO_OGIS_DATE2, :OPEN_OGIS2, :CLOSE_OGIS2, :O' +
        'GIS_RETURN_DATE2)')
    RefreshSQL.Strings = (
      'Select '
      '  J.UKEY,'
      '  J.ACCOUNT,'
      '  J.REG_NUMBER,'
      '  J.REG_DATE,'
      '  J.REG_DEBT,'
      '  J.FIO_ALL,'
      '  J.SUIT_DEBT,'
      '  J.DATE_FROM,'
      '  J.DATE_TILL,'
      '  J.LAW_TELL0,'
      '  J.LAW_TELL,'
      '  J.SUMA,'
      '  J.CLOSE_LAW,'
      '  J.TO_OGIS,'
      '  J.EXECUTIVE,'
      '  J.OPEN_OGIS,'
      '  J.VERDICT_OGIS,'
      '  J.CLOSE_OGIS,'
      '  J.DESCRIPTION,'
      '  J.LOCKED,'
      '  J.LAST_USER,'
      '  J.LAST_DATE,'
      '  J.LAST_TIME,'
      '  J.TO_OGIS_DATE,'
      '  J.CLOSE_LAW1,'
      '  J.OGIS_RETURN_DATE,'
      '  (SELECT SALDOOUT FROM GET_ACCOUNT_SALDOOUT_LAST(J.ACCOUNT)) '
      'AS SALDOOUT,'
      '  (SELECT ADDRESS FROM GET_ACCOUNT_ADDRESS(J.ACCOUNT)) AS '
      'ADDRESS,'
      'J.TO_OGIS_DATE2, '
      'J.OPEN_OGIS2, '
      'J.CLOSE_OGIS2, '
      'J.OGIS_RETURN_DATE2'
      ''
      'from JOURNAL J'
      'where'
      '  UKEY = :UKEY')
    SelectSQL.Strings = (
      'SELECT'
      '       J.UKEY, J.ACCOUNT, J.REG_NUMBER, J.REG_DATE,'
      '       J.REG_DEBT, J.FIO_ALL, J.SUIT_DEBT, J.DATE_FROM,'
      '       J.DATE_TILL, J.LAW_TELL0, J.LAW_TELL, J.SUMA,'
      '       J.CLOSE_LAW, J.TO_OGIS, J.EXECUTIVE, J.OPEN_OGIS,'
      '       J.VERDICT_OGIS, J.CLOSE_OGIS, J.DESCRIPTION, J.LOCKED,'
      '       J.LAST_USER, J.LAST_DATE, J.LAST_TIME, J.TO_OGIS_DATE,'
      '       J.CLOSE_LAW1,'
      '       J.OGIS_RETURN_DATE,'
      
        '       (SELECT SALDOOUT FROM GET_ACCOUNT_SALDOOUT_LAST(J.ACCOUNT' +
        ')) AS SALDOOUT,'
      
        '       (SELECT ADDRESS FROM GET_ACCOUNT_ADDRESS(J.ACCOUNT)) AS A' +
        'DDRESS,'
      
        '       J.TO_OGIS_DATE2, J.OPEN_OGIS2, J.CLOSE_OGIS2, J.OGIS_RETU' +
        'RN_DATE2'
      'FROM'
      '       JOURNAL J'
      'WHERE'
      '      J.UKEY=:UKEY')
    ModifySQL.Strings = (
      'update JOURNAL'
      'set'
      '  ACCOUNT = :ACCOUNT,'
      '  REG_NUMBER = :REG_NUMBER,'
      '  REG_DATE = :REG_DATE,'
      '  REG_DEBT = :REG_DEBT,'
      '  FIO_ALL = :FIO_ALL,'
      '  SUIT_DEBT = :SUIT_DEBT,'
      '  DATE_FROM = :DATE_FROM,'
      '  DATE_TILL = :DATE_TILL,'
      '  LAW_TELL0 = :LAW_TELL0,'
      '  LAW_TELL = :LAW_TELL,'
      '  SUMA = :SUMA,'
      '  CLOSE_LAW = :CLOSE_LAW,'
      '  TO_OGIS = :TO_OGIS,'
      '  EXECUTIVE = :EXECUTIVE,'
      '  OPEN_OGIS = :OPEN_OGIS,'
      '  VERDICT_OGIS = :VERDICT_OGIS,'
      '  CLOSE_OGIS = :CLOSE_OGIS,'
      '  DESCRIPTION = :DESCRIPTION,'
      '  LOCKED = :LOCKED,'
      '  LAST_USER = :LAST_USER,'
      '  LAST_DATE = :LAST_DATE,'
      '  LAST_TIME = :LAST_TIME,'
      '  TO_OGIS_DATE=:TO_OGIS_DATE,'
      '  CLOSE_LAW1=:CLOSE_LAW1,'
      '  OGIS_RETURN_DATE=:OGIS_RETURN_DATE,'
      '  TO_OGIS_DATE2=:TO_OGIS_DATE2,'
      '  OPEN_OGIS2=:OPEN_OGIS2, '
      '  CLOSE_OGIS2=:CLOSE_OGIS2, '
      '  OGIS_RETURN_DATE2=:OGIS_RETURN_DATE2'
      'where'
      '  UKEY = :OLD_UKEY')
    Left = 736
