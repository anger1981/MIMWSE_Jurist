      'where'
      '  ID = :OLD_ID')
    InsertSQL.Strings = (
      'insert into ZMVK_JOURNAL_DETAIL_1'
      '  (JOURNAL, FIO, REMARK, VERDICT_OGIS0, VERDICT_OGIS, FIRM, '
      'TO_FIRM_DATE)'
      'values'
      
        '  (:JOURNAL, :FIO, :REMARK, :VERDICT_OGIS0, :VERDICT_OGIS, :FIRM' +
        ', '
      ':TO_FIRM_DATE)')
